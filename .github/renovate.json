{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "ignorePaths": [],
  "regexManagers": [
    {
      "fileMatch": [
        "src/test/java/.+\\.java$"
      ],
      "matchStrings": [
        "\"(?<depName>.*?):(?<currentValue>.*?)\""
      ],
      "datasourceTemplate": "docker"
    }
  ]
}
